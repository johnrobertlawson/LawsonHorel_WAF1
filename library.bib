issn = {0022-4928},
journal = {J. Atmos. Sci.},
month = sep,
number = {9},
pages = {2539--2558},
title = {{The Basic Ingredients of the North Atlantic Storm Track. Part I: Land–Sea Contrast and Orography}},
url = {http://journals.ametsoc.org/doi/abs/10.1175/2009JAS3078.1},
volume = {66},
year = {2009}
}
