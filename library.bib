@article{Woollings2008,
abstract = {Abstract This paper proposes the hypothesis that the low-frequency variability of the North Atlantic Oscillation (NAO) arises as a result of variations in the occurrence of upper-level Rossby wave–breaking events over the North Atlantic. These events lead to synoptic situations similar to midlatitude blocking that are referred to as high-latitude blocking episodes. A positive NAO is envisaged as being a description of periods in which these episodes are infrequent and can be considered as a basic, unblocked situation. A negative NAO is a description of periods in which episodes occur frequently. A similar, but weaker, relationship exists between wave breaking over the Pacific and the west Pacific pattern. Evidence is given to support this hypothesis by using a two-dimensional potential-vorticity-based index to identify wave breaking at various latitudes. This is applied to Northern Hemisphere winter data from the 40-yr ECMWF Re-Analysis (ERA-40), and the events identified are then related to the NAO. Cert...},
author = {Woollings, Tim and Hoskins, Brian and Blackburn, Mike and Berrisford, Paul},
doi = {10.1175/2007JAS2347.1},
file = {:home/jrlawson/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Woollings et al. - 2008 - A New Rossby Wave–Breaking Interpretation of the North Atlantic Oscillation.pdf:pdf},
issn = {0022-4928},
journal = {J. Atmos. Sci.},
keywords = {NAO,North Atlantic Oscillation,Potential vorticity,Rossby waves,Teleconnections,Wave breaking,rossby wave breaking},
language = {EN},
mendeley-tags = {NAO,rossby wave breaking},
month = feb,
number = {2},
pages = {609--626},
title = {{A New Rossby Wave–Breaking Interpretation of the North Atlantic Oscillation}},
